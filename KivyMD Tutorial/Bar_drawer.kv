<Root>
    MDNavigationLayout:
        MDScreenManager:
            Screen:
                BoxLayout:
                    size:root.size
                    orientation:'vertical'
                    MDTopAppBar:
                        title:'TopBar'
                        left_action_items:[["menu",lambda x: nav_drawer.toggle_nav_drawer()]]
                        #right_action_items:[['clock'],["dots-vertical"]]
                        elevation:5
                    Widget:
        MDNavigationDrawer:
            id:nav_drawer
